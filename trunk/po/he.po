msgid "<b>P6</b>: 60&#176; rotation"
msgstr "<b>P6</b>: סיבוב של 60&#176;"

#: ../src/dialogs/clonetiler.cpp:1929
msgid "<b>P6M</b>: reflection + 60&#176; rotation"
msgstr "<b>P6M</b>: השתקפות + סיבוב של 60&#176;"

