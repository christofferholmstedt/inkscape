# Pedro mini-XMPP client, used for Inkboard's Jabber functionality
# Author: Bob Jamison

pedro/all: pedro/libpedro.a
pedro/clean: 
	rm -f pedro/libpedro.a $(pedro_libpedro_a_OBJECTS)

pedro_SOURCES= \
	pedro/pedroconfig.cpp \
	pedro/pedroconfig.h \
	pedro/pedrodom.cpp \
	pedro/pedrodom.h \
	pedro/pedrogui.cpp \
	pedro/pedrogui.h \
	pedro/pedroutil.cpp \
	pedro/pedroutil.h \
	pedro/pedroxmpp.cpp \
	pedro/pedroxmpp.h

if WITH_INKBOARD
temp_pedro_files = $(pedro_SOURCES)
endif

pedro_libpedro_a_SOURCES = \
	pedro/empty.cpp \
	$(temp_pedro_files)
