msgstr "<b>P1</b>: jednoduchý posuv"

#: ../src/dialogs/clonetiler.cpp:1675
msgid "<b>P2</b>: 180&#176; rotation"
msgstr "<b>P2</b>: rotace o 180&#176;"

#: ../src/dialogs/clonetiler.cpp:1676
msgid "<b>PM</b>: reflection"
msgstr "<b>PM</b>: zrcadlení"

