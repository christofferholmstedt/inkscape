msgid "<b>PMM</b>: reflection + reflection"
msgstr "<b>PMM</b>: riflessione + riflessione"

#: ../src/dialogs/clonetiler.cpp:1912
msgid "<b>PMG</b>: reflection + 180&#176; rotation"
msgstr "<b>PMG</b>: riflessione + rotazione 180&#176;"

