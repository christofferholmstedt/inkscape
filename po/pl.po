msgid "<b>P3</b>: 120&#176; rotation"
msgstr "<b>P3</b>: obrót o 120&#176;"

#: ../src/dialogs/clonetiler.cpp:1669
msgid "<b>P31M</b>: reflection + 120&#176; rotation, dense"
msgstr "<b>P31M</b>: odbicie + obrót o 120&#176;, gęsto"

