msgid "<b>CMM</b>: reflection + reflection + 180&#176; rotation"
msgstr "<b>CMM</b>: odbicie + odbicie + obrót o 180&#176;"

#: ../src/dialogs/clonetiler.cpp:1665
msgid "<b>P4</b>: 90&#176; rotation"
msgstr "<b>P4</b>: obrót o 90&#176;"

