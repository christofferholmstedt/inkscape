msgid "<b>Angle:</b>"
msgstr "<b>Кут:</b>"

#: ../src/dialogs/clonetiler.cpp:2277
#, no-c-format
msgid "Rotate tiles by this angle for each row"
msgstr "Обертати плитки на цей кут на кожен рядок"

