msgid "<b>P2</b>: 180&#176; rotation"
msgstr "<b>P2</b>: обертання на 180&#176;"

#: ../src/dialogs/clonetiler.cpp:1734
msgid "<b>PM</b>: reflection"
msgstr "<b>PM</b>: віддзеркалення"

